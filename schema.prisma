generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider  = "postgresql"
    url       = env("POSTGRES_PRISMA_URL") // uses connection pooling
    directUrl = env("POSTGRES_URL_NON_POOLING") // uses a direct connection
}

model advisory {
    id                 Int    @id @default(autoincrement())
    recommendationType String
    fieldId            BigInt
    value              String
}
